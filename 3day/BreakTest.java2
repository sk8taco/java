package j200117;

public class BreakTest {

	public static void main(String[] args) {
		System.out.println("레이블이 없는 브레이크문");
		for(int a=0; a<5;a++) {
			for(int b=0;b<5;b++) {
				if(b==2) {
					break;
				}
				System.out.println("a="+a+",b="+b); 
			}//inner for
			System.out.println("===============");
		}//outer for
		System.out.println("이중 for문 종료");
	}

}
